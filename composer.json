{
    "name": "makinacorpus/drupal-tooling",
    "type" : "library",
    "description": "Minimalist Drupal custom console and toolset",
    "homepage" : "http://github.com/makinacorpus/drupal-tooling",
    "license" : "GPL-2",
    "authors": [
        {
            "name": "Pierre Rineau",
            "email": "pierre.rineau@makina-corpus.com"
        },
        {
            "name": "Makina Corpus",
            "homepage": "http://makina-corpus.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MakinaCorpus\\DrupalTooling\\": "src"
        },
        "exclude-from-classmap": [
            "/src/Tests/"
        ]
    },
    "require" : {
        "php": ">=5.6",
        "psr/log" : "1.*",
        "symfony/config" : "~2.8|~3.0",
        "symfony/console": "~2.8|~3.0",
        "symfony/dependency-injection" : "~3.0",
        "symfony/event-dispatcher" : "~2.8|~3.0",
        "symfony/expression-language" : "~2.8|~3.0",
        "symfony/http-foundation" : "~2.8|~3.0",
        "symfony/http-kernel" : "~2.8|~3.0",
        "symfony/translation": "~2.8|~3.0",
        "symfony/yaml" : "~2.8|~3.0"
    },
    "suggest" : {
        "makinacorpus/drupal-sf-dic" : "For embeding the Symfony kernel into your Drupal 7 site",
        "symfony/finder": "For potential bundles to be able to register commands"
    },
    "minimum-stability" : "dev"
}
